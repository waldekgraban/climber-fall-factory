{
    "name": "waldekgraban/climber-fall-factory",
    "description":"The fall factor is the ratio of the height at which the climber falls before the climber rope begins to stretch, and the length of the rope available to absorb the fall energy.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Waldemar Graban",
        "email": "waldekgraban@gmail.com"
    }],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.2",
        "phpunit/phpunit": "^8.5",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Waldekgraban\\ClimberFallFactory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waldekgraban\\ClimberFallFactory\\": "src/",
            "Waldekgraban\\ClimberFallFactory\\Tests\\": "tests/"
        }
    }
}